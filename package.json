{
  "name": "vue-portfolio",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "run-p type-check \"build-only {@}\" -- && vite build && node scripts/generate-sitemap.cjs",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.12",
    "bootstrap-icons": "^1.13.1",
    "swiper": "^11.2.10",
    "vue": "^3.5.18",
    "vue-i18n": "^11.1.11",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.16.5",
    "@types/swiper": "^5.4.3",
    "@types/vue-i18n": "^6.1.3",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "~10.3.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^8.0.4",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "sass": "^1.90.0",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.8.0",
    "vite": "^7.0.6",
    "vue-tsc": "^3.0.4"
  }
